{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "newLine": "LF",
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "allowJs": true
  }
}
